{
  "name": "watchestime-ecommerce",
  "version": "1.0.0",
  "description": "E-commerce website for watches with JWT authentication",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
